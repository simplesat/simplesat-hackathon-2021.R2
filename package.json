{
  "name": "simplesat-hackathon-2021.r2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"yarn tailwind:watch\" \"next dev\"",
    "build": "yarn tailwind:build && next build",
    "start": "next start",
    "lint": "tsc && next lint",
    "tailwind:watch": "npx tailwindcss -i styles/tailwind.config.css -o styles/tailwind.css --watch",
    "tailwind:build": "npx tailwindcss -i styles/tailwind.config.css -o styles/tailwind.css",
    "storybook": "concurrently \"yarn tailwind:watch\" \"start-storybook -p 9000\"",
    "build-storybook": "yarn tailwind:build && build-storybook",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/client": "^3.3.21",
    "@emotion/css": "^11.1.3",
    "@emotion/react": "^11.4.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/pro-regular-svg-icons": "^5.15.3",
    "@fortawesome/pro-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@xstate/react": "^1.5.1",
    "classnames": "^2.3.1",
    "graphql": "^15.5.1",
    "immer": "^9.0.5",
    "next": "11.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "xstate": "^4.22.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/react": "^6.3.4",
    "@welldone-software/why-did-you-render": "^6.2.0",
    "autoprefixer": "^10.3.1",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.2.0",
    "eslint": "7.30.0",
    "eslint-config-next": "11.0.1",
    "firebase": "^8.7.1",
    "husky": "^7.0.1",
    "postcss": "^8.3.5",
    "react-firebaseui": "^5.0.2",
    "tailwindcss": "^2.2.4",
    "twin.macro": "^2.6.2",
    "typescript": "^4.3.5"
  },
  "babelMacros": {
    "twin": {
      "preset": "emotion"
    }
  }
}
